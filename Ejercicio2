import java.util.Arrays;

public class Ejercicio2 {
    public static void main(String[] args) {
        long[] primerArreglo = new long[10];
        
        for (int i = 0; i < primerArreglo.length; i++) {
            primerArreglo[i] = i * 20;
        }

        long[] segundoArreglo = Arrays.copyOf(primerArreglo, primerArreglo.length);

        System.out.println("Elementos del segundo arreglo:");
        for (int i = 0; i < segundoArreglo.length; i++) {
            System.out.println("Elemento " + i + ": " + segundoArreglo[i]);
        }
    }
}
